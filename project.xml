<?xml version="1.0" encoding="utf-8"?>
<project>
	<!-- NMML reference: https://gist.github.com/1763850 -->
	
	<!-- metadata, make sure 'package' is at least 3 segments (ie. com.mycompany.myproject) -->
	<meta title="CaveGenerator" package="CaveGenerator" version="1.0.0" company="Elemental Code" />
	
	<!-- output -->
	<app main="Main" file="CaveGenerator" path="bin" />
	
	<window background="#333333" fps="60" />
	<window width="640" height="360" unless="mobile" resizable="false" />
	<window orientation="landscape" vsync="false" antialiasing="0" if="cpp" />
	
	<!-- classpath, haxe libs -->
	<source path="src" />
	<haxelib name="openfl" />

	
	<!-- assets -->
	
	<!-- optimize output -->
	<haxeflag name="-dce full" />
	
</project>